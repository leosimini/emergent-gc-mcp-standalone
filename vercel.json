{
  "version": 2,
  "name": "gastoscompartidos-mcp-server",
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "src/**",
        "logs/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
