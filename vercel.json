{
  "version": 2,
  "name": "gastoscompartidos-mcp-server",
  "routes": [
    { "src": "/(.*)", "dest": "src/server.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/server.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30,
      "includeFiles": ["src/**"]
    }
  }
}
